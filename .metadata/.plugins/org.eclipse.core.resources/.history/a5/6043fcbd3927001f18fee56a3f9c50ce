
package donnees;

public class Tarif 
{
	private Spectacle spectacle;
	private Zone zone;
	private double pleinTarif;
	
	public Tarif(Spectacle s, Zone z) 
	{
		this.spectacle = s;
		this.zone = z;
	}

	public double getPleinTarif() {
		return pleinTarif;
	}

	public void setPleinTarif(double pleinTarif) {
		this.pleinTarif = pleinTarif;
	}
	
	
}
